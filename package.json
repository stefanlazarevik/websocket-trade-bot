{
  "name": "socktrader",
  "version": "0.0.0-development",
  "description": "Websocket based trading bot for cryptocurrencies",
  "repository": "https://github.com/SockTrader/SockTrader",
  "license": "GPL-3.0",
  "keywords": [
    "tradingbot",
    "algorithmic-trading",
    "cryptocurrencies",
    "binance",
    "trading-algorithms",
    "trading-bot",
    "trading",
    "crypto",
    "bitcoin"
  ],
  "workspaces": [
    "packages/**",
    "apps/**"
  ],
  "scripts": {
    "clean": "nx reset; npx rimraf dist node_modules **/node_modules",
    "build:all": "nx run-many --target=build --all",
    "build:affected": "nx affected:build",
    "lint:all": "nx run-many --target=lint --all --fix",
    "lint:affected": "nx affected:lint --fix",
    "test:all": "cross-env TZ=UTC nx run-many --target=test --verbose --all --parallel",
    "test:affected": "cross-env TZ=UTC nx affected:test --verbose",
    "start:backtest": "nx serve demo",
    "start:web": "nx serve web"
  },
  "dependencies": {
    "@ngneat/elf": "^1.5.6",
    "@ngneat/elf-entities": "^4.3.0",
    "binance-api-node": "^0.11.35",
    "commander": "^9.2.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "express": "^4.17.3",
    "nanoid": "^3.3.2",
    "pg": "^8.7.3",
    "rxjs": "7.5.5",
    "tslib": "^2.3.1",
    "technicalindicators": "^3.1.0"
  },
  "devDependencies": {
    "@nrwl/cli": "13.10.2",
    "@nrwl/eslint-plugin-nx": "13.10.2",
    "@nrwl/jest": "13.10.2",
    "@nrwl/js": "13.10.2",
    "@nrwl/linter": "13.10.2",
    "@nrwl/node": "13.10.2",
    "@nrwl/workspace": "13.10.2",
    "@types/config": "0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "~5.18.0",
    "cross-env": "^7.0.3",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-rxjs": "^5.0.2",
    "jest": "27.5.1",
    "nx": "13.10.2",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "~4.6.3",
    "semantic-release": "^19.0.2"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "authors": [
    "wouter cleymans",
    "thijs raets"
  ]
}
