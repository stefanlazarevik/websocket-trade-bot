{
    "branches": [
        "master"
    ],
    "plugins": [
        [
            "@semantic-release/commit-analyzer",
            {
                "preset": "ESLint",
            }
        ],
        [
            "@semantic-release/release-notes-generator",
            {
                "preset": "ESLint"
            }
        ],
        [
            "@semantic-release/exec",
            {
                "publishCmd": "nx run-many --target=release --all --args=\"--version=${nextRelease.version}\""
            }
        ],
        "@semantic-release/github"
    ]
}
